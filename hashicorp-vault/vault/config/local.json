{
  "listener": [{
    "tcp": {
      "address": "0.0.0.0:8200",
      "tls_cert_file": "/vault/certs/vault.crt",
      "tls_key_file": "/vault/certs/vault.key"
    }
  }],
  "storage": {
    "file": {
      "path": "/vault/data"
    }
  },
  "default_lease_ttl": "168h",
  "max_lease_ttl": "720h",
  "ui": true,
  "log_level": "INFO",
  "api_addr": "https://vault:8200",
  "disable_mlock": true
}

